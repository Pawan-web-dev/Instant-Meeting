{
    
    "version": 2,
    "functions": {
    "src/app.js": {
      "runtime": "edge"
    }
  },
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    
    "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/app.js",
      "headers": { "Access-Control-Allow-Origin": "*" }
    }
  ]
}




  
