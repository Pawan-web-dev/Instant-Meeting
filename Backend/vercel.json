{
  "version": 2,
  "functions": {
    "src/app.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/src/app.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Connection": "keep-alive"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/src/app.js"
    }
  ]
}
